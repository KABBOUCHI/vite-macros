{
  "name": "vite-macros",
  "version": "0.0.7",
  "description": "",
  "repository": "KABBOUCHI/vite-macros",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "types.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
