    "prepack": "yarn build",
    "test": "yarn lint && vitest run --coverage"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.31.4",
    "changelogen": "^0.5.3",
    "eslint": "^8.42.0",
    "eslint-config-unjs": "^0.2.0",
    "fs": "^0.0.1-security",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5",
    "unbuild": "^1.2.1",
    "vite": "^4.4.3",
    "vitest": "^0.31.4"
  }
}
